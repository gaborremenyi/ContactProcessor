@model ContactProcessor.Models.NewContactViewModel

@{
    ViewBag.Title = "Write";
}
<h2>Write</h2>

<form action="" method="post">

    @Html.HiddenFor(x => x.FileName)

    <table>
        <tr>
            <td><b>First Name:</b></td>
            <td>@Html.TextBoxFor(x => x.FirstName)
            </td>
        </tr>
        <tr>
            <td><b>Last Name:</b></td>
            <td>@Html.TextBoxFor(x => x.LastName)
            </td>
        </tr>
        <tr>
            <td><b>Phone Number:</b></td>
            <td>@Html.TextBoxFor(x => x.PhoneNumber)
            </td>
        </tr>
        <tr>
            <td><b>Email:</b></td>
            <td>@Html.TextBoxFor(x => x.Email)
            </td>
        </tr>
    </table>

    <input type="submit" value="Append"/>

</form>